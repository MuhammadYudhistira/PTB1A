<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailLogbook">

    <TextView
        android:id="@+id/labelNamalogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Nama: "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/labelNIMlogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="NIM:  "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labelNamalogbook" />

    <TextView
        android:id="@+id/labelTempatlogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Tempat: "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labelNIMlogbook" />

    <TextView
        android:id="@+id/DetailLogbookCatatan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="50dp"
        android:text="Catatan"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labelTanggallogbook" />

    <TextView
        android:id="@+id/DetailLogbookRespon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="200dp"
        android:text="Respon"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DetailLogbookCatatan" />

    <Button
        android:id="@+id/btnRespon"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="32dp"
        android:onClick="ResponClicked"
        android:text="Respon"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/tvCatatan"
        android:layout_width="370dp"
        android:layout_height="170dp"
        android:text="Mencari-cari cari cari cari"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/DetailLogbookCatatan"
        app:layout_constraintTop_toBottomOf="@+id/DetailLogbookCatatan" />

    <TextView
        android:id="@+id/labelTanggallogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Tanggal:"
        app:layout_constraintStart_toStartOf="@+id/labelTempatlogbook"
        app:layout_constraintTop_toBottomOf="@+id/labelTempatlogbook" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Detail Logbook"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvRespon"
        android:layout_width="371dp"
        android:layout_height="170dp"
        android:text="Tingkatkan"
        app:layout_constraintEnd_toEndOf="@+id/tvCatatan"
        app:layout_constraintStart_toStartOf="@+id/DetailLogbookRespon"
        app:layout_constraintTop_toBottomOf="@+id/DetailLogbookRespon" />

    <TextView
        android:id="@+id/namaDetailLogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="Muhammad Yudhistira"
        app:layout_constraintStart_toEndOf="@+id/labelNamalogbook"
        app:layout_constraintTop_toTopOf="@+id/labelNamalogbook" />

    <TextView
        android:id="@+id/nimDetailLogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2011523003"
        app:layout_constraintStart_toStartOf="@+id/namaDetailLogbook"
        app:layout_constraintTop_toTopOf="@+id/labelNIMlogbook" />

    <TextView
        android:id="@+id/tempatDetailLogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Google"
        app:layout_constraintStart_toStartOf="@+id/nimDetailLogbook"
        app:layout_constraintTop_toTopOf="@+id/labelTempatlogbook" />

    <TextView
        android:id="@+id/tanggalDetailLogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senin, 6 September 2022"
        app:layout_constraintStart_toStartOf="@+id/tempatDetailLogbook"
        app:layout_constraintTop_toTopOf="@+id/labelTanggallogbook" />

</androidx.constraintlayout.widget.ConstraintLayout>